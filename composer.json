{
    "name": "seaonye/laravel-valorant-api",
    "description": "a valorant api wrapper for php",
    "license": "MIT",
    "authors": [
        {
            "name": "seaony",
            "email": "seaony@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": ">=7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Seaony\\ValorantApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seaony\\ValorantApi\\ValorantServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --colors"
    },
    "scripts-descriptions": {
        "test": "Run all tests."
    }
}
